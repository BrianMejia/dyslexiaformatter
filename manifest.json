{
  "name": "Div Highlight",
  "description": "Change the current page",
  "version": "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*", "storage"
  ],
  "browser_action": {
      "default_title": "Highlight divs using the mouse.",
      "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["core.css"],
      "js": ["jquery-3.1.1.min.js", "core.js", "options.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "manifest_version": 2
}
